		"typescript": "^2.8.3",
		"webpack": "^4.6.0",
		"webpack-cli": "^2.0.15",
		"webpack-dev-server": "^3.1.3"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/egorovsa/react-video-seek-slider.git"
